<div class="container">
  <div class="row justify-content-center">
    <div class="col-6">
      <h1>Create flat</h1>
      <%= simple_form_for(@flat) do |f| %>
        <%= f.input :address_line_1 %>
        <%= f.input :postcode %>
        <%= f.input :city %>
        <%= f.input :price %>
        <%= f.submit %>
      <% end %>
    </div>
  </div>
</div>


